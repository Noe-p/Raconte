@font-face {
  font-family: "Text-menu";
  src: url("469c4356b412ab8915cdeddf66dda4fd");
}
.menu {
  background-color: white;
  color: #121a27;
}
.reportage {
  padding-top: 100px;
  padding-bottom: 100px;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.reportage h1 {
  font-family: "Text-menu";
  font-size: 5vw;
  margin: 0 10px 0 0;
  padding-left: 20px;
  border-bottom: solid 1px #121a27;
  text-align: left;
  width: 90%;
}
.reportage h1:hover {
  background-color: #121a27;
  color: white;
  cursor: pointer;
}