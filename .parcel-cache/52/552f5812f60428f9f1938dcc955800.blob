@font-face {
  font-family: "Titre";
  src: url("f4d56cbd9c9bcd72e90aeae339143963");
}
@font-face {
  font-family: "Titre2";
  src: url("836e07b89114e20540a70202e7b17bd1");
}
@font-face {
  font-family: "Text-medium";
  src: url("ac0ee252243d91605bc71cbd4c137d8d");
}
@font-face {
  font-family: "Text-regular";
  src: url("86bb13f9d28923712907e0d34735441a");
}
body {
  margin: 0;
  padding: 0;
  font-size: 1.2em;
  font-family: "Text-regular";
}
.navbar.hidden, .main.hidden {
  opacity: 0;
}
.loader {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: teal;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  z-index: 100;
}
.loader.hidden {
  display: none;
}