body {
  margin: 0;
  padding: 0;
  font-size: 1.2em;
  font-family: "Text-regular";
}
.navbar.hidden, .main.hidden, .stickman.hidden {
  opacity: 0;
}
.loader {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  z-index: 100;
  opacity: 1;
}
.loader h1 {
  color: black;
  font-family: "Titre";
  font-size: 10vw;
  display: block;
  letter-spacing: -1vw;
}
.loader h1 span {
  animation-name: letter-wave;
  animation-iteration-count: infinite;
  animation-duration: 1.5s;
  animation-delay: 1s;
  position: relative;
  top: 0;
  opacity: 1;
}
@keyframes letter-wave {
  20% {
    top: -25%;
  }
}
.loader .load {
  display: block;
  content: "";
  background-color: teal;
  height: 50px;
  width: 50px;
  border-radius: 50%;
  position: absolute;
  left: 50%;
  top: 20%;
  animation-name: load;
  animation-duration: 3s;
  animation-timing-function: ease-in;
  animation-iteration-count: infinite;
}
@keyframes load {
  0% {
    background-color: orange;
  }
  33% {
    background-color: purple;
  }
  66% {
    background-color: brown;
  }
}
.loader.hidden {
  opacity: 0;
  transform: translate(100%, -100%);
  animation-name: loader-end;
  animation-duration: 1s;
  animation-timing-function: ease-in;
}
@keyframes loader-end {
  0% {
    transform: translate(0, 0);
    opacity: 1;
    z-index: 100;
    border-radius: 0;
  }
  100% {
    transform: translateY(-100%);
    opacity: 1;
    z-index: 100;
    border-bottom-left-radius: 100%;
    border-bottom-right-radius: 100%;
  }
}
