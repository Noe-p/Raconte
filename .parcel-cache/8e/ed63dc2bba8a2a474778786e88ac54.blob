aside {
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: rgba(255, 255, 255, 0);
  height: 7vh;
  position: fixed;
  top: 0;
  z-index: 90;
  width: 100%;
}
aside a {
  font-family: "Text";
  color: white;
  text-decoration: none;
}
aside a:hover {
  cursor: pointer;
}

.navbar.white a {
  color: #121a27;
}
.navbar.white a:hover::before {
  border: solid 1px #121a27;
}
.navbar.white a:active::before {
  background-color: #121a27;
}

.item_nav_left {
  background-color: teal;
}
.item_nav_left a {
  margin-left: 30px;
  margin-right: 30px;
  background-color: brown;
}

.item_nav_index {
  position: absolute;
  left: 30px;
}
.item_nav_index::before {
  position: absolute;
  content: "";
  display: block;
  border: solid 0px white;
  left: 50%;
  top: 50%;
  width: 0;
  height: 0;
  border-radius: 50%;
  transition: all 0.3s;
}
.item_nav_index:hover::before {
  border: solid 1px white;
  top: -6px;
  left: -15%;
  width: 130%;
  height: 30px;
  border-radius: 30px;
}
.item_nav_index:active::before {
  background-color: white;
}