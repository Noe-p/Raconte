.ay {
  display: block;
  position: relative;
  top: 0;
  opacity: 1;
  background-color: white;
  color: #121a27;
  transition: all 1s ease-out;
  transition-delay: 0.3s;
}

.ay.hidden {
  opacity: 0;
  top: 150px;
}
