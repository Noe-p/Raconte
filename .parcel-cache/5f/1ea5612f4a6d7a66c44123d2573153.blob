body {
  margin: 0;
  padding: 0;
  font-size: 1em;
  font-family: "Text";
}
.loader {
  position: absolute;
  width: 100vw;
  height: 100vh;
  background-color: white;
  overflow-x: hidden;
  overflow-y: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  z-index: 100;
  opacity: 1;
}
.loader h1 {
  color: black;
  font-family: "Titre";
  font-size: 10vw;
  display: block;
  letter-spacing: -1vw;
}
.loader h1 span {
  animation-name: letter-wave;
  animation-iteration-count: infinite;
  animation-duration: 4s;
  position: relative;
  top: 0;
  opacity: 1;
}
.loader h1 span:nth-child(2) {
  animation-delay: 0.2s;
}
.loader h1 span:nth-child(3) {
  animation-delay: 0.4s;
}
.loader h1 span:nth-child(4) {
  animation-delay: 0.6s;
}
.loader h1 span:nth-child(5) {
  animation-delay: 0.8s;
}
.loader h1 span:nth-child(6) {
  animation-delay: 1s;
}
.loader h1 span:nth-child(7) {
  animation-delay: 1.2s;
}
@keyframes letter-wave {
  10% {
    top: -25%;
  }
  20% {
    top: 0;
  }
}
.loader.hidden {
  opacity: 0;
  transform: translate(100%, -100%);
  animation-name: loader-end;
  animation-duration: 1s;
  animation-timing-function: ease-in;
}
.loader.open {
  animation-name: loader-open;
  animation-duration: 1s;
  animation-timing-function: ease-in;
  transform: translate(0, 0);
  opacity: 1;
  z-index: 100;
  border-radius: 0;
}
@keyframes loader-end {
  0% {
    transform: translate(0, 0);
    opacity: 1;
    z-index: 100;
    border-radius: 0;
  }
  100% {
    transform: translateY(-100%);
    opacity: 1;
    z-index: 100;
    border-bottom-left-radius: 100%;
    border-bottom-right-radius: 100%;
  }
}
@keyframes loader-open {
  0% {
    transform: translateY(-100%);
    opacity: 1;
    z-index: 100;
    border-bottom-left-radius: 100%;
    border-bottom-right-radius: 100%;
  }
  100% {
    transform: translate(0, 0);
    opacity: 1;
    z-index: 100;
    border-radius: 0;
  }
}
@media all and (max-device-width: 480px) {
  body {
    width: auto;
  }
}
