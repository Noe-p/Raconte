@font-face {
  font-family: "Titre";
  src: url("ViaodaLibre-Regular.HASH_REF_ff8fce525dcf492036df21e977245b56.ttf");
}
@font-face {
  font-family: "Text-bold";
  src: url("Montserrat-Black.HASH_REF_0d49371e52eaeedfaa286fb406b5829d.ttf");
}
@font-face {
  font-family: "Text-regular";
  src: url("Roboto-Regular.HASH_REF_62ef1bac088248c4f844659fb201c50d.ttf");
}
@font-face {
  font-family: "Text";
  src: url("Montserrat-Regular.HASH_REF_727c11bf10a17da9a01999f1d529a939.ttf");
}
body {
  margin: 0;
  padding: 0;
  font-size: 1em;
  font-family: "Text";
}
.navbar.hidden, .content.hidden, .stickman.hidden, .menu.hidden {
  display: none;
}
.loader {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  z-index: 100;
  opacity: 1;
}
.loader h1 {
  color: black;
  font-family: "Titre";
  font-size: 10vw;
  display: block;
  letter-spacing: -1vw;
}
.loader h1 span {
  animation-name: letter-wave;
  animation-iteration-count: infinite;
  animation-duration: 4s;
  position: relative;
  top: 0;
  opacity: 1;
}
.loader h1 span:nth-child(2) {
  animation-delay: 0.2s;
}
.loader h1 span:nth-child(3) {
  animation-delay: 0.4s;
}
.loader h1 span:nth-child(4) {
  animation-delay: 0.6s;
}
.loader h1 span:nth-child(5) {
  animation-delay: 0.8s;
}
.loader h1 span:nth-child(6) {
  animation-delay: 1s;
}
.loader h1 span:nth-child(7) {
  animation-delay: 1.2s;
}
@keyframes letter-wave {
  10% {
    top: -25%;
  }
  20% {
    top: 0;
  }
}
.loader.hidden {
  opacity: 0;
  transform: translate(100%, -100%);
  animation-name: loader-end;
  animation-duration: 1s;
  animation-timing-function: ease-in;
}
@keyframes loader-end {
  0% {
    transform: translate(0, 0);
    opacity: 1;
    z-index: 100;
    border-radius: 0;
  }
  100% {
    transform: translateY(-100%);
    opacity: 1;
    z-index: 100;
    border-bottom-left-radius: 100%;
    border-bottom-right-radius: 100%;
  }
}
