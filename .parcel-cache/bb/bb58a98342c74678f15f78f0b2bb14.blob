  font-family: "Titre";
  src: url("d442b25000c397dd710a3bafb5fd0e36");
}
@font-face {
  font-family: "Titre2";
  src: url("e671012ed11843e934e39e60a8be85de");
}
@font-face {
  font-family: "Text-medium";
  src: url("adec4a567e9fa43aac7655f3a08a828d");
}
@font-face {
  font-family: "Text-regular";
  src: url("5302c7a2e5a2a0bbffa950d10929441d");
}
body {
  margin: 0;
  padding: 0;
  font-size: 1.2em;
  font-family: "Text-regular";
}
.navbar.hidden, .main.hidden {
  opacity: 0;
}
.loader {
  position: absolute;
  width: 100%;
  height: 100%;
  background-color: teal;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  z-index: 100;
}
.loader.hidden {
  display: none;
}